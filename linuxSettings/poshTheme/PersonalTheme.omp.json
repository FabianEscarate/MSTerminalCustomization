{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "time",
          "style": "plain",
          "foreground": "#ffffff",
          "template": "[{{ .CurrentDate | date .Format }}]"
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "lightYellow",
          "template": " {{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }} ",
          "properties": {
            "display_host": false,
            "display_user": true
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "cyan",
          "template": "<#ffffff>in</> {{ .Path }} ",
          "properties": {
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#ff94df",
          "template": "<#ffffff>on</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "lightGreen",
          "template": "‚ùØ "
        }
      ]
    }
  ]
}
